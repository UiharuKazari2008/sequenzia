                                                            else if (thisClass.uri === 'cards')
                                                                i.fas.fa-sticky-note.ml-1
                                                            else if (thisSuperClass.uri === 'gallery')
                                                                i.fas.fa-photo-video.ml-1
                                                            else if (thisSuperClass.uri === 'files')
                                                                i.fas.fa-folder.ml-1
                                                            else if (thisSuperClass.uri === 'cards')
                                                                i.fas.fa-sticky-note.ml-1
