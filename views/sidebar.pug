                                                            div.d-inline-flex(style="position: absolute;right: 1.5rem;margin-top: 0.2rem;")
                                                                if (thisChannel.nsfw)
                                                                    i.fas.fa-exclamation-triangle(style="color: #ff4a4a;").mr-1
