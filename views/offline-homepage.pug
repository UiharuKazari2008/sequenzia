        script.
            if (location.hash.startsWith('#/tvTheater') || location.hash.startsWith('#/listTheater')) {
                document.getElementById('bootUpDisplay').style.display = 'none';
                document.getElementById('bootUpDisplay').querySelector('.boot-status-holder').removeAttribute('id');
                document.getElementById('bootUpDisplay').querySelector('.boot-status-holder').innerText = "JuneOS Platform v20"
                document.getElementById('bootUpDisplay').querySelector('.boot-icons-holder').removeAttribute('id');
                document.getElementById('bootUpDisplay').querySelector('.boot-icons-holder').classList.add('d-none');

                document.getElementById('kmsBootDisplay').style.display = 'flex';
                document.getElementById('kmsBootDisplay').querySelector('.boot-status-holder').setAttribute('id', 'bootLoaderStatus');
                document.getElementById('kmsBootDisplay').querySelector('.boot-status-holder').innerText = "Waiting for core..."
                document.getElementById('kmsBootDisplay').querySelector('.boot-icons-holder').classList.remove('d-none');
